{
	"compilerOptions": {
		"target": "ES2021",
		"lib": [
			"ES2021",
		],
		"module": "Node16",
		"sourceMap": true,
		"composite": true,
		"declaration": true,
		"strict": true,
		"alwaysStrict": false,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
	},
}