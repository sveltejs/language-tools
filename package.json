    "devDependencies": {
        "@types/cosmiconfig": "^4.0.0",
        "@types/detect-indent": "^5.0.0",
        "@types/indent-string": "^3.0.0",
        "@types/mocha": "^5.0.0",
        "@types/node": "^8.10.7",
        "@types/prettier": "^1.12.0",
        "@types/sinon": "^4.3.1",
        "@types/source-map": "^0.5.7",
