    },
    "type": "commonjs",
    "main": "index.js",
    "types": "index.d.ts",
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^8.10.53",
        "@types/parse5": "^5.0.2",
        "@types/unist": "^2.0.3",
        "@types/vfile": "^3.0.2",
        "magic-string": "^0.25.4",
        "mocha": "^6.2.2",
        "parse5": "^5.1.0",
        "rollup": "^1.12.0",
        "rollup-plugin-commonjs": "^10.0.0",
        "rollup-plugin-delete": "^1.1.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-typescript": "^1.0.1",
        "source-map": "^0.6.1",
        "source-map-support": "^0.5.16",
        "svelte": "3.23.0",
